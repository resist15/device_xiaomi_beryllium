android_app {
    name: "XiaomiSettings",
    defaults: [
        "SettingsLibDefaults",
    ],

    srcs: ["src/**/*.java"],
    resource_dirs: ["res"],
    certificate: "platform",
    platform_apis: true,

    static_libs: [
        "co.aospa.resources",
        "SettingsLib",
    ],

    optimize: {
        proguard_flags_files: ["proguard.flags"],
    },
}
